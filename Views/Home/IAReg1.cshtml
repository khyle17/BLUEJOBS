
@{
    ViewBag.Title = "Industry/Agency";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="swim-content">
        <div class="container">
            <div>
                <center>
                    <h1>Create an Account</h1><br />
                                              <p>Upload Business Permit:</p>
                    <p><input type="file" id="txtCode" name="txtCode" /></p><br />
                   
                </center>
                <div>
                    <p align="center"><input onclick="location.href='@Url.Action("IADash")'" class="btn btn-primary btn-md" value="Create Account" /></p>
                    <p align="center"><input onclick="location.href='@Url.Action("IAReg")'" class="btn btn-primary btn-md" value="Back" /></p>
                </div>


            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>
